package day03;

public class 형변환 {

	public static void main(String[] args) {
		
		
		//형 : 자료형 (type)
		
		// 형변환 : type을 변환하는 것
		// -자동형변환 (묵시적)
		// -강제형변환 (명시적)
		
		
		byte b = 127;		// -128 ~ 127
		
		byte c = (byte) 128; //형을 강제로 변환 시킴 		=> 오버플로우 발생
		System.out.println(c);

		
		
		int su1=10;
		int su2=3;
		
		double result = su1/su2;  // 10 / 3
		
		System.out.println(result);
		
	}

}
